@page
@model EntAppSecond.Pages.Students.CreateModel
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@{
    ViewData["Title"] = "Create";
}



<h2>Create</h2>

<form method="post">
    <div>
        <label asp-for=Student.FirstName></label>
        <input asp-for=Student.FirstName>
    </div>

    <div>
        <label asp-for=Student.LastName></label>
        <input asp-for=Student.LastName>
    </div>

    <div>
        <label asp-for=Student.StudentID></label>
        <input asp-for=Student.StudentID>

        @*<div asp-validation-summary="All"></div>*@
        <Span asp-validation-for="Student.StudentID"></Span>

    </div>

    <div>
        <label asp-for=Student.DOB></label>
        <input asp-for=Student.DOB>

        @*<div asp-validation-summary="All"></div>*@
        @*<Span asp-validation-for="Student.DOB"></Span>*@

    </div>

    <div>
        <label asp-for=Student.NoOfModules></label>
        <input asp-for=Student.NoOfModules>

        @*<div asp-validation-summary="All"></div>*@
        <Span asp-validation-for="Student.NoOfModules"></Span>

    </div>

    <div>
        <label asp-for=Student.Height></label>
        <input asp-for=Student.Height>

        @*<div asp-validation-summary="All"></div>*@
        <Span asp-validation-for="Student.Height"></Span>

    </div>

    <div>
        <label asp-for=Student.Email></label>
        <input asp-for=Student.Email />
    </div>

    <div>
        <label asp-for=Student.EmailConfirmation></label>
        <input asp-for=Student.EmailConfirmation />
    </div>

    <button type="submit">Press Me</button>
</form>




